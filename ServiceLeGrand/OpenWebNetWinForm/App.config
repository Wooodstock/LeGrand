<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <behaviors>
      <serviceBehaviors>
        <behavior name="metadata">
          <serviceMetadata httpGetEnabled="true" />
        </behavior>
      </serviceBehaviors>
    </behaviors>
    <services>
      <service behaviorConfiguration="metadata" name="ServiceLegrand.ServiceLegrand">
        <endpoint address="http://localhost:8019/OpenWebNet" binding="basicHttpBinding"
          contract="InterfaceServiceLegrand.IServiceLegrand" />
        <host>
          <baseAddresses>
            <add baseAddress="http://localhost:8030/ServiceOpenWebNet" />
          </baseAddresses>
        </host>
      </service>
    </services>
  </system.serviceModel>
</configuration>